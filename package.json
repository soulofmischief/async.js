{
  "name": "@soulofmischief/async.js",
  "version": "1.1.2",
  "description": "An advanced Javascript async library.",
  "author": "soulofmischief <soulofmischief@protonmail.com>",
  "repository": "https://github.com/soulofmischief/async.js",
  "license": "MIT",
  "engines": {
    "node": ">=8.5.0"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run clean --scripts-prepend-node-path; for script in {flow,lib}; do npm run build:$script --scripts-prepend-node-path; done",
    "build:dev": "NODE_ENV=development nodemon -w src --exec 'npm run build'",
    "build:flow": "npx flow-copy-source src lib",
    "build:lib": "babel src -d lib",
    "clean": "rimraf lib",
    "lint:flow": "npx flow status",
    "lint:flow:stop": "npx flow stop",
    "prepublishOnly": "npm run build --scripts-prepend-node-path",
    "test": "babel-node node_modules/.bin/ospec"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "@soulofmischief/array.js": "^0.5.1",
    "@soulofmischief/js-utils": "^0.5.3",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-flow": "^7.0.0",
    "@istanbuljs/nyc-config-babel": "^2.1.1",
    "babel-eslint": "^10.0.3",
    "babel-plugin-istanbul": "^5.2.0",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^6.5.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "flow-bin": "^0.109.0",
    "flow-copy-source": "^2.0.8",
    "nyc": "^14.1.1",
    "ospec": "^4.0.1",
    "rimraf": "^3.0.0"
  }
}
